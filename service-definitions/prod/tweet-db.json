{
  "name": "tweet-db",
  "image": "postgres",
  "keywordArgs": [
    "-d",
    "-t",
    "--constraint node.hostname==mimir-2",
    "--mount type=volume,source=tweet-db-data,destination=/var/lib/postgresql/data"
  ],
  "envVars": [
    "POSTGRES_PASSWORD=$PG_PASSWORD",
    "POSTGRES_USER=simon",
    "POSTGRES_DB=mimirprod"
  ]
}
