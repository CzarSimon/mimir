{
  "name": "news-pg",
  "image": "postgres",
  "keywordArgs": [
    "-d",
    "-t",
    "--mount type=volume,source=news-pg-data,destination=/var/lib/postgresql/data",
    "--constraint 'node.hostname == mimir-u1'"
  ],
  "envVars": [
    "POSTGRES_PASSWORD=$PG_PASSWORD",
    "POSTGRES_USER=simon",
    "POSTGRES_DB=mimirprod"
  ]
}
