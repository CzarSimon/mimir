{
  "name": "stream-listener",
  "image": "czarsimon/stream-listener",
  "keywordArgs": [
    "-d",
    "-t",
    "--replicas=1"
  ],
  "envVars": [
    "PG_HOST=tweet-pg",
    "PG_PASSWORD=$PG_PASSWORD",
    "app_server_ip=app-server",
    "ranker_ip=news-ranker",
    "spam_filter=spam-filter",
    "HANDLE_SPAM=TRUE"
  ]
}
